#!/bin/bash
echo Getting VODs data...
latestvodinfo=$(./twitch-dl.1.22.0.pyz videos paymoneywubby)
echo Finding newest VOD...
latestvodidlong=$(echo $latestvodinfo | grep -Po 'Video \K\d+')
echo Extracting VOD ID...
latestvodid=$(echo $latestvodidlong | awk '{print $1;}')
echo Starting downloader...
./twitch-dl.1.22.0.pyz download -q source $latestvodid --output "{date}_{title_slug}.{format}"
